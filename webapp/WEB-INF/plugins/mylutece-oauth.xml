<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
   <name>mylutece-oauth</name>
   <class>fr.paris.lutece.plugins.mylutece.modules.oauth.service.OAuthPlugin</class>
   <version>1.1.0</version>
   <description>module.mylutece.oauth.module.description</description>
   <documentation/>
   <installation/>
   <changes/>
   <user-guide/>
   <provider>module.mylutece.oauth.module.provider</provider>
   <provider-url>http://lutece.paris.fr</provider-url>
   <icon-url>images/admin/skin/plugins/mylutece/mylutece.png</icon-url>
   <copyright>Copyright 2001-2010 Mairie de Paris</copyright>
   <core-version-dependency>
		<min-core-version>2.4.1</min-core-version>
		<max-core-version/>	
   </core-version-dependency>
	
	<!-- if the plugin must have a connection pool with parameter : 1 - yes, 0 - no -->   
   <db-pool-required>1</db-pool-required>
    
   <!-- Administration interface parameters -->
   <admin-features>
     <admin-feature>
       <feature-id>OAUTH_MANAGEMENT</feature-id>
       <feature-title>module.mylutece.oauth.adminFeature.oauth_management.name</feature-title>
       <feature-description>module.mylutece.oauth.adminFeature.oauth_management.description</feature-description>
       <feature-level>3</feature-level>
       <feature-url>jsp/admin/plugins/mylutece/modules/oauth/ManageOAuth.jsp</feature-url>
     </admin-feature>
    </admin-features>
   <daemons>
   </daemons>
   
</plug-in>